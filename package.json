{
  "name": "gallery-platform",
  "version": "1.0.0",
  "description": "Multilingual image/video gallery platform with TTL-driven configuration",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "node --watch src/server.js",
    "start": "NODE_ENV=production node src/server.js",
    "migrate": "node scripts/migrate.js",
    "import:photopost": "node scripts/import-photopost.js",
    "generate:schema": "node scripts/generate-schema.js",
    "validate:ttl": "node scripts/validate-ttl.js",
    "build:frontend": "vite build",
    "dev:frontend": "vite",
    "build:creatives": "vite build --config vite.config.creatives.js",
    "dev:creatives": "vite --config vite.config.creatives.js",
    "spider:chaturbate": "node scripts/run-spider.js chaturbate",
    "spider:bongacams": "node scripts/run-spider.js bongacams",
    "spider:stripchat": "node scripts/run-spider.js stripchat",
    "spider:onlyfans": "node scripts/run-browser-spider.js onlyfans",
    "spider:fansly": "node scripts/run-browser-spider.js fansly",
    "spider:all-api": "node scripts/run-spider.js all",
    "import:performers": "node scripts/import-performers.js"
  },
  "dependencies": {
    "@google-cloud/translate": "^9.3.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "geoip-lite": "^1.4.10",
    "helmet": "^7.1.0",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.0",
    "mysql2": "^3.16.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-reddit": "^1.1.0",
    "passport-twitter": "^1.0.4",
    "pg": "^8.11.3",
    "playwright": "^1.57.0",
    "react-i18next": "^16.5.0",
    "sharp": "^0.33.2",
    "translate-google-api": "^1.0.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "vite": "^5.0.10"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "gallery",
    "rdf",
    "ttl",
    "multilingual",
    "photopost",
    "streaming"
  ],
  "author": "",
  "license": "MIT"
}
